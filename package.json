  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^5.1.0",
    "figlet": "^1.4.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}

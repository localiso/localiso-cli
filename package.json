{
  "name": "localiso-cli",
  "version": "0.0.1",
  "description": "Localiso CLI",
  "main": "./lib/index.js",
  "bin": {
    "localiso-cli": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
